{
	"name": "Markdown Viewer",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Markdown预览工具",
	"browser_action": {
		"default_icon": "favicon.png"
	},
	"content_scripts": [{
		"matches": ["file://*/*.md"],
		"js": [
			"index.js",
			"content-script.js"
		],
		"css": [
			"css/layout.css",
			"css/code-styles/github.css",
			"css/markdown.css"
		]
	}],
	"permissions": [
		"activeTab"
	]
}
