{
	"name": "EX1 App",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "这是一个测试用例",
	"browser_action": {
		"default_icon": "logo1.png",
		"default_popup": "popup.html",
		"default_title": "Click here!"
	},
	"content_scripts": [{
		"matches": ["file://*/*.md"],
		"js": [
			"index.js",
			"content-script.js"
		],
		"css": [
			"css/code-styles/default.css"
		]
	}],
	"permissions": [
		"activeTab"
	]
}
